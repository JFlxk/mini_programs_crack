    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    display: flex;
    z-index: 1000;
    overflow: visible;
}

.page .mainTabBar .alike {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 100%;
    font-size: 20rpx;
    font-family: PingFangSC-Medium,PingFang SC,NotoSansSC-Medium,Noto Sans SC;
    font-weight: 400;
    color: #666;
}

.page .mainTabBar .alike .iconfont {
    font-size: 48rpx;
}

.page .mainTabBar .recording {
    position: relative;
    top: 0;
}

.page .mainTabBar .recording .recording-icon {
    display: inline-block;
    width: 102rpx;
    height: 102rpx;
    text-align: center;
    background: linear-gradient(312deg,#5a82ff,#3badff);
    border-radius: 50%;
}

.page .mainTabBar .recording .recording-icon .iconfont {
    font-size: 45rpx;
    color: #fff;
    display: inline-block;
    margin-top: 1rpx;
}

.page .mainTabBar .recording .recording-icon .recording-text {
    font-size: 20rpx;
    font-family: PingFangSC-Medium,PingFang SC,NotoSansSC-Medium,Noto Sans SC;
